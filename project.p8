pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	evpos={{1,9},{3,9},{5,9},{11,9},{13,9},{1,6},{3,6},{5,6},{11,6},{13,6},{1,12},{3,12},{5,12},{11,12},{13,12}}
	x=6
	y=6
	day_time = 1
	remaining_ev = 0
	solved_ev = 0
	day = 1
	pause = false
	day_over = false
	summary_screen = false
	day_start = time()
end
function _update()
		player_pos = {flr(x+7), flr(y+8)} // global var of player
		if (not pause) then
			_solveevent()
		elseif (time() - time_paused >= timer) then
			pause = false
		end
		to_the_right = fget(mget(x+9,y+8))
		to_the_left = fget(mget(x+6,y+8))
		to_the_top = fget(mget(x+8,y+7))
		to_the_bottom = fget(mget(x+8,y+9))
		if (pause) then
			speed = 0
		elseif (btn(5)) then
			speed = 0.3
		else
			speed = 0.2
		end
		if day_over then
			speed = 0
		end
		if (btn(1) and to_the_right != 2) x+=speed
		if (btn(0) and to_the_left != 2) x-=speed
		if (btn(2) and to_the_top != 2) y-=speed
		if (btn(3) and to_the_bottom != 2) y+=speed
end
function _draw()
	cls()
	if not summary_screen then
		palt(0, false)
		palt(3, true)
		map(x,y, 0,0, 16,16,0)
		if  (not pause) then
			if (flr(x)==6 and flr(y)==6) then
				spr(110, 56, 56, 2, 2)
			else
				spr(2, 56,56 , 2, 2)
			end
		else
		 spr(108, 56, 56, 2, 2)
		end
		if day_over then
			if player_pos[2] == 1 then
				c = 7
			else
				c = 0
			end
			print("day over!", 50, 50, c)
			if (time() - day_start >= day_time + 5) then
				summary_screen = true
			end
		end
		_gameloop()
	else
		print("day: " .. day, 20, 16, 7)
		print("solved events: " .. solved_ev, 20, 32, 7)
		print("unsolved events: " .. remaining_ev, 20, 48, 7)
		if btn(4) then
				next_day()
		end
	end
end
function _gameloop()
if not day_over then
	_startday()
end
if (time()-day_start >= day_time) then
	day_over = true
end
end

function _createevent()

end

function _startday()
if (time() >= 5 and time() % 3 == 0) then
	if (flr(rnd(100)) >= 40) then
		pos=rnd(evpos)
		remaining_ev += 1
		mset(pos[1], pos[2], 23 + rnd(2))
	end
end
stam = 100
end

function _solveevent()
	get_event = mget(player_pos[1], player_pos[2]) // gets the sprite at the player's pos
	get_flag = fget(get_event) // gets the flag of the sprite
	timers = {3, 6, 9} // different times for each event
	timer = timers[get_event-22] // offset the sprites to get the timer for the event
	if (get_flag==4) then //checks if event pos is equal to player pos
		if (btn(4) and not pause) then
			pause = true
			remaining_ev -= 1
			solved_ev += 1
			mset(player_pos[1], player_pos[2], 4)
			time_paused = time()
		end	
	end
end

function next_day()
	evpos={{1,9},{3,9},{5,9},{11,9},{13,9},{1,6},{3,6},{5,6},{11,6},{13,6},{1,12},{3,12},{5,12},{11,12},{13,12}}
	x=6
	y=6
	day += 1
	pause = false
	day_over = false
	summary_screen = false
	day_start = time()
end
